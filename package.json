{
  "name": "nbs-root",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run build:prod",
    "dev": "cross-env ENV_PROFILE=development webpack serve --config scripts/webpack.config.js",
    "build:prod": "cross-env ENV_PROFILE=production webpack --config scripts/webpack.config.js",
    "build:staging": "cross-env ENV_PROFILE=staging webpack --config scripts/webpack.config.js",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/eslint-parser": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
    "@strv/eslint-config-react": "^6.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "babel-loader": "^10.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-webpack-plugin": "^5.0.2",
    "globals": "^17.0.0",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.7.4",
    "react-refresh": "^0.18.0",
    "sass": "^1.97.2",
    "sass-loader": "^16.0.6",
    "simple-git-hooks": "^2.13.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3",
    "webpack-manifest-plugin": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "simple-git-hooks": {
    "commit-msg": "npx --no-install commitlint --edit $1"
  }
}
